<!-- We remove all html,head,title cause in layout folder in boilerplate.ejs   -->
<!-- we use a library fot this in all page header,footer are same we don't need to write again and again -->
<!-- only we use as in line (4) -->
<% layout("/layouts/boilerplate") %>

    <h1>All Listings</h1>
    <!-- <form method="GET" action="/listings/new">
        <button>Create New Listing</button>
    </form> -->
    <div class="row row-cols-lg-3">
      <% for(let listing of allListings) { %>
        <div class="card col" style="width: 20rem;">
            <img src="<%= listing.image %>" class="card-img-top" alt="listing_Img">
            <div class="card-body">
              <p class="card-text">
                <%= listing.title %></p><br>
                &#8377; <%= listing.price.toLocaleString("en-IN") %> /night
            </div>
          </div>
        <% } %>  
    </div>

  